{
  "manifest_version": 2,

  "name": "Pitch Shift",
  "description": "This extension will shift the pitch the browser's audio (HTML5)",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts":["background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js":      ["content.js"]
  }],
  "permissions": [
    "activeTab"
  ]
}